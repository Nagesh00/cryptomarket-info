{
  "name": "crypto-project-monitor",
  "version": "1.0.0",
  "description": "Real-time crypto project monitoring system with multi-source data collection and notifications",
  "main": "production-server.js",
  "scripts": {
    "start": "node production-server.js",
    "dev": "node simple-server.js",
    "production": "NODE_ENV=production node production-server.js",
    "simple": "node simple-server.js",
    "test": "jest",
    "docker:build": "docker build -t crypto-monitor .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "heroku-postbuild": "npm install",
    "build": "npm run build:api && npm run build:extension",
    "build:api": "webpack --config webpack.api.config.js",
    "build:extension": "cd vscode-extension && npm run compile",
    "setup": "npm install && cd vscode-extension && npm install",
    "powershell": "powershell -ExecutionPolicy Bypass -File CryptoProfile.ps1",
    "setup-powershell": "powershell -ExecutionPolicy Bypass -File Setup-PowerShellIntegration.ps1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.0",
    "cheerio": "^1.0.0-rc.12",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "natural": "^6.12.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "rate-limiter-flexible": "^2.4.2",
    "redis": "^4.6.11",
    "sentiment": "^5.0.2",
    "socket.io": "^4.7.4",
    "telegram-bot-api": "^2.0.1",
    "web-push": "^3.6.6",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "cryptocurrency",
    "blockchain",
    "monitoring",
    "notifications",
    "real-time",
    "trading",
    "analysis"
  ],
  "author": "Crypto Monitor Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nagesh00/cryptomarket-info.git"
  },
  "bugs": {
    "url": "https://github.com/Nagesh00/cryptomarket-info/issues"
  },
  "homepage": "https://github.com/Nagesh00/cryptomarket-info#readme"
}
