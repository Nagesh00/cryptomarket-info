{
  "name": "advanced-crypto-project-monitor",
  "version": "2.0.0",
  "description": "Comprehensive real-time cryptocurrency project monitoring system with AI analysis, dark web intelligence, and multi-channel notifications",
  "main": "advanced-crypto-monitor.js",
  "scripts": {
    "start": "node deployment-crypto-monitor.js",
    "start:full": "node advanced-crypto-monitor.js",
    "dev": "NODE_ENV=development node advanced-crypto-monitor.js",
    "monitor": "node crypto-project-monitor.js",
    "analyze": "python crypto_analyzer.py",
    "darkweb": "python dark_web_monitor.py",
    "setup": "python setup.py",
    "test": "npm run test:node && npm run test:python",
    "test:node": "node -e \"console.log('Node.js test passed')\"",
    "test:python": "python -c \"print('Python test passed')\"",
    "build": "npm install",
    "production": "NODE_ENV=production node deployment-crypto-monitor.js",
    "simple": "node simple-server.js",
    "extension": "cd vscode-extension && npm install && npm run compile",
    "heroku-postbuild": "npm install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.3",
    "axios": "^1.11.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "helmet": "^7.2.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "socket.io": "^4.7.4",
    "telegram-bot-api": "^2.0.1",
    "discord.js": "^14.14.1",
    "sentiment": "^5.0.2",
    "natural": "^6.12.0",
    "compromise": "^14.10.0",
    "redis": "^4.6.11",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^2.4.2",
    "web-push": "^3.6.6",
    "bullmq": "^4.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "cryptocurrency",
    "blockchain",
    "monitoring",
    "notifications",
    "real-time",
    "trading",
    "analysis",
    "AI",
    "dark-web",
    "intelligence",
